{
  "name": "mcp-camara-br",
  "version": "1.0.0",
  "description": "Servidor MCP completo para API da CÃ¢mara dos Deputados do Brasil",
  "type": "module",
  "main": "dist/server.js",
  "bin": {
    "mcp-camara-br": "./dist/server.js"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:http": "tsx watch src/http-server.ts",
    "dev:sse": "tsx watch src/mcp-sse-server.ts",
    "build": "tsc",
    "build:windows": "npm run build",
    "build:unix": "npm run build && chmod +x dist/server.js",
    "build:clean": "npm run clean && npm run build",
    "start": "node dist/server.js",
    "start:http": "node dist/http-server.js",
    "start:sse": "node dist/mcp-sse-server.js",
    "clean": "rimraf dist",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run format:check && npm test",
    "generate:types": "tsx scripts/generate-types.ts",
    "update:cache": "tsx scripts/update-cache.ts",
    "docker:build": "docker build -t mcp-camara-br .",
    "docker:run": "docker-compose up",
    "docker:stop": "docker-compose down"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "camara",
    "brasil",
    "deputados",
    "legislativo",
    "opendata",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gvc2000/AgenteCidadaoMCP.git"
  },
  "bugs": {
    "url": "https://github.com/gvc2000/AgenteCidadaoMCP/issues"
  },
  "homepage": "https://github.com/gvc2000/AgenteCidadaoMCP#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    ".env.example"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@types/cors": "^2.8.19",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "lru-cache": "^10.0.0",
    "p-queue": "^7.4.0",
    "p-retry": "^6.0.0",
    "pino": "^8.16.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.52.0",
    "nock": "^13.4.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.2.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
